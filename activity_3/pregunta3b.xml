for $b in /response/row/row
let $mx := max(confirmed_cases)
where $b/residence = "No"
return
<COVIDData>
  <AgrupatedData Isinresidences="{$b/residence}" MeasurementDate="{$b/start_date}">
    <LleidaData>
      <ConfirmedCases>{$b/confirmed_cases}</ConfirmedCases>
      <ConfirmedCasesRate>{$b/confirmed_cases_rate}</ConfirmedCasesRate>
    </LleidaData>
    <TGNData>
      <ConfirmedCases>{$b/confirmed_cases}</ConfirmedCases>
      <ConfirmedCasesRate>{$b/confirmed_cases_rate}</ConfirmedCasesRate>
    </TGNData>
    <BCNData>
      <ConfirmedCases>{$b/confirmed_cases}</ConfirmedCases>
      <ConfirmedCasesRate>{$b/confirmed_cases_rate}</ConfirmedCasesRate>
    </BCNData>
    <GironaData>
     <ConfirmedCases>{$b/confirmed_cases}</ConfirmedCases>
     <ConfirmedCasesRate>{$b/confirmed_cases_rate}</ConfirmedCasesRate>
    </GironaData>
  </AgrupatedData>
  <IsolatedData>
   <LleidaData>
    <ConfirmedCases></ConfirmedCases>
   </LleidaData>
   <TGNData>
    <ConfirmedCases></ConfirmedCases>
   </TGNData>
   <BCNData>
    <ConfirmedCases></ConfirmedCases>
   </BCNData>
   <GironaData>
    <ConfirmedCases></ConfirmedCases>
   </GironaData>
  </IsolatedData>
</COVIDData>
